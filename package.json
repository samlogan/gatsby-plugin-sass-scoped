    "sass",
    "scss"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^2.0.0",
    "node-sass": "^4.9.0"
  },
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-plugin-sass"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
