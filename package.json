{
  "name": "gatsby-plugin-sass-scoped",
  "description": "Gatsby plugin to handle scoped scss/sass files",
  "version": "1.0.0",
  "author": "Sam Logan <sam@woollymammoth.co",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "babel-plugin-react-scoped-css": "^1.0.0",
