    "babel-preset-gatsby-package": "^0.2.16",
    "cross-env": "^5.2.1"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-plugin-sass#readme",
  "keywords": [
